{
  "name": "@swaphub/backend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "ts-node-dev --rs ./src/index.ts",
    "build": "rimraf ./dist && tsc --build ./tsconfig.json",
    "start": "node ./dist/index.js",
    "types": "tsc --noEmit --project ./tsconfig.json",
    "prettify": "prettier --log-level warn --cache --write \"src/**/*.{ts,tsx,js,json,yml,scss}\" \"*.{ts,tsx,js,json,yml,scss}\" --ignore-path .gitignore",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts ."
  },
  "dependencies": {
    "@trpc/server": "^11.8.1",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "lodash": "^4.17.23"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.0.3"
  }
}
