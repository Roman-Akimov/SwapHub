{
  "name": "@swaphub/webswaphub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json && tsc --noEmit --project ./tsconfig.app.json",
    "prettify": "prettier --log-level warn --cache --write \"src/**/*.{ts,tsx,js,json,yml,scss}\" \"*.{ts,tsx,js,json,yml,scss}\" --ignore-path .gitignore",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache src/**/*.scss"
  },
  "dependencies": {
    "@swaphub/backend": "workspace:*",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.80.3",
    "@trpc/client": "^11.9.0",
    "@trpc/react-query": "^11.9.0",
    "include-media": "^2.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "reset-css": "^5.0.2",
    "sass": "^1.97.3",
    "sass-embedded": "^1.97.2",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "8.57.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "stylelint": "^17.2.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
